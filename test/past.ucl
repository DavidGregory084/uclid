module main
{
    var n : integer;
    var inited : boolean;

    init {
        assume (n > 0);
        inited = false;
    }
    
    next {
        n' = n - 1;
        inited' = true;
    }

    invariant n_decreases: inited ==> (n < past(n));

    control {
        v = unroll(10);
        check;
        v.print_results;
        // v.print_cex(n);
    }
}

